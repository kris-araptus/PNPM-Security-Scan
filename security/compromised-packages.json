    "protestware": {
      "description": "Packages intentionally sabotaged by maintainers as a form of protest. Impact varies from annoying to destructive, but these are not traditional malware designed to steal data.",
      "high": {
        "description": "Destructive protestware that can delete or corrupt files",
        "packages": [
          "node-ipc",
          "peacenotwar"
        ],
        "details": {
          "node-ipc": "March 2022: Added code to overwrite files with heart emojis on systems with Russian/Belarusian IP addresses (Ukraine protest). Destructive behavior.",
          "peacenotwar": "Dependency of node-ipc with same file-overwriting behavior targeting Russian/Belarusian IPs."
        }
      },
      "medium": {
        "description": "Protestware causing denial of service (infinite loops, crashes) but no data loss",
        "packages": [
          "colors",
          "faker"
        ],
        "details": {
          "colors": "January 2022: Maintainer added infinite loop printing 'LIBERTY LIBERTY LIBERTY' as protest against corporations. Causes application hang, no data theft.",
          "faker": "January 2022: Same maintainer, same protest. Infinite loop causing DoS. Package deprecated, use @faker-js/faker instead."
        }
      },
      "low": {
        "description": "Protestware with messages or minor annoyances, no functional impact",
        "packages": [
          "es5-ext"
        ],
        "details": {
          "es5-ext": "Added console message about Ukraine war. No malicious functionality, just a political statement in output."
        }
      }
    },
    
